{
  "name": "api",
  "version": "1.0.0-pre.56",
  "main": "dist/index.js",
  "scripts": {
    "dev": "npm test && npm run dev-no-test",
    "dev-no-test": "ts-node-dev --respawn --pretty --transpile-only src/index.ts",
    "start": "NODE_ENV=production node dist/index.js",
    "build": "npm test && tsc",
    "bun-dev": "bun test && bun run bun-dev-no-test",
    "bun-dev-no-test": "bun run --watch src/index.ts",
    "bun-start": "NODE_ENV=production bun run dist/index.js",
    "bun-build": "bun test && bun run ./node_modules/typescript/lib/tsc.js",
    "test": "jest --verbose"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/csps/api.git"
  },
  "keywords": [
    "nodejs",
    "typescript",
    "org",
    "bun",
    "bunjs"
  ],
  "author": "csps",
  "license": "other",
  "bugs": {
    "url": "https://github.com/csps/api/issues"
  },
  "homepage": "https://github.com/csps/api#readme",
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.3",
    "@types/mysql": "^2.15.21",
    "@types/node": "^20.4.8",
    "@types/nodemailer": "^6.4.8",
    "chalk": "^4.1.2",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "jose": "^4.14.4",
    "mysql": "^2.18.1",
    "nodemailer": "^6.9.4"
  },
  "overrides": {
    "semver": "~7.5.2"
  }
}
